@{
    ViewData["Title"] = "Ergebnis";
}

@model WissenstestOnlineWebseite.Models.Ergebnis_Model



<div id="results" class="container">
    <h3>Egebnis Übersicht</h3>

    <h4>Maximal erreichbare Punkte: @Model.Max_Punkte</h4>
    <h4>Erreichte Punkte: @Model.Punkte</h4>
    <h4>Ereichte Punkte in Prozent: @Model.ProzentRichtig%</h4><br />
    <a class="btn btn-primary" role="button" id="closeResults">Zurück zur Stationsauswahl</a>
</div>

@section scripts{
    <environment names="Development">
        <script src="~/js/site.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="~/js/site.min.js"></script>
    </environment>
}