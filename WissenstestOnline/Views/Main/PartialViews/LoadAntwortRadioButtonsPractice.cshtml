@model WissenstestOnlineWebseite.Models.AntwortRadioButtons_Model

<div class="container" id="allRadioButtons">

    @foreach (var radioButton in @Model.RadioButtons)
    {
        <div class="radio" id="radioButtons">
            <label id="rb_@radioButton.Radio_Id"><input type="radio" name="gruppe" id="@radioButton.Radio_Id" value="@radioButton.Content" class="radioButtonInput" /> @radioButton.Content</label>
        </div>
    }

</div>