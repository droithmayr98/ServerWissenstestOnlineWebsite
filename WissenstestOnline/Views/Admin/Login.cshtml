@{
    ViewData["Title"] = "FF Wissenstest OÖ";
}


<div class="container">
    <div class="row">
        <div class="col-md-4">    <!-- col-md-offset-4 -->
            <form id="login" action="index.html" class="well">
                <div class="form-group">
                    <label>Username</label>
                    <input type="text" class="form-control" placeholder="Enter Username" id="adminUsername">
                </div>
                <div class="form-group">
                    <label>Password</label>
                    <input type="password" class="form-control" placeholder="Enter Passowrd" id="adminPasswort">
                </div>
                <button type="button" class="btn btn-default" id="adminLoginButton">Login</button>
            </form>
            <p id="LoginError" style="color: red"></p>
        </div>
    </div>
</div>

@section scripts{

    <script src="~/js/admin.js"></script>
}