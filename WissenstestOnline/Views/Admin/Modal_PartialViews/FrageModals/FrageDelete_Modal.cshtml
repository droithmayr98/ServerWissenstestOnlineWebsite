@model WissenstestOnlineWebseite.AdminModels.FrageInfo_Model

<!--FrageDelete-->
<div class="modal fade" id="frageDelete_Modal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header MyDangerHeader">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">FrageDelete: @Model.FrageText</h4>
            </div>
            <div class="modal-body">
                <p><span style="font-weight: bold">Frage ID:</span> <span id="frage_id_delete">@Model.FrageId</span></p>
                <h5>Soll diese Frage wirklich entfernt werden?</h5>
                <span style="font-weight: bold">@Model.FrageText</span>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal" id="deleteFrage">Delete</button>
            </div>
        </div>

    </div>
</div>
