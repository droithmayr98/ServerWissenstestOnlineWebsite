@model WissenstestOnlineWebseite.AdminModels.ZusatzInfo_InfoModel

<!--AntwortDelete-->
<div class="modal fade" id="zusatzInfoDelete_Modal" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header MyDangerHeader">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">ZusatzInfoDelete: @Model.ZusatzInfo_Name</h4>
            </div>
            <div class="modal-body">
                <p><span style="font-weight: bold">ZusatzInfo ID:</span> <span id="zusatzInfo_id_delete">@Model.ZusatzInfo_Id</span></p>
                <h5>Soll diese ZusatzInfo wirklich entfernt werden?</h5>
                <span style="font-weight: bold">@Model.ZusatzInfo_Name</span>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal" id="deleteZusatzInfo">Delete</button>
            </div>
        </div>

    </div>
</div>
